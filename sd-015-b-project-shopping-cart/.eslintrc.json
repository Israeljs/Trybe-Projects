{
  "extends": "trybe-fundamentals",
  "rules": {
    "no-alert": "off",
    "no-console": "off",
    "prefer-arrow-callback": "off",
    "func-names": "off",
    "no-unused-vars": "warn"
  },
  "env": {
    "browser": true
  },
  "no-use-before-define": ["error", { "functions": true, "classes": true }]
}
